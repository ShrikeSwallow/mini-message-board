<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <li><span>User</span><span>Text</span><span>Date</span></li>
      <% messages.forEach(message => { %>
      <li>
        <span><%= message.user %></span><span><%= message.text %></span
        ><span><%= message.added %></span>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
